{
  "name": "@kb-labs/plugin-adapter-cli",
  "version": "0.1.3",
  "description": "CLI adapter for KB Labs plugins, handling command execution and discovery",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-pattern 'dist/**'",
    "lint:fix": "eslint . --fix --ignore-pattern 'dist/**'",
    "test": "vitest run -c ../../vitest.config.ts",
    "test:watch": "vitest -c ../../vitest.config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.929.0",
    "@kb-labs/rest-api-contracts": "link:../../../../kb-labs-rest-api/packages/rest-api-contracts",
    "@kb-labs/cli-contracts": "link:../../../../kb-labs-cli/packages/cli-contracts",
    "@kb-labs/cli-core": "link:../../../../kb-labs-cli/packages/cli-core",
    "@kb-labs/core": "link:../../../../kb-labs-core",
    "@kb-labs/core-bundle": "link:../../../../kb-labs-core/packages/core-bundle",
    "@kb-labs/core-sys": "link:../../../../kb-labs-core/packages/core-sys",
    "@kb-labs/core-sandbox": "link:../../../../kb-labs-core/packages/core-sandbox",
    "@kb-labs/plugin-manifest": "link:../../plugin-manifest",
    "@kb-labs/plugin-runtime": "link:../../plugin-runtime",
    "@kb-labs/shared-cli-ui": "link:../../../../kb-labs-shared/packages/shared-cli-ui",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "execa": "^9.6.0",
    "glob": "^11.0.0",
    "minimatch": "^10.0.1",
    "picomatch": "^4.0.2",
    "uuidv7": "^1.0.0",
    "yaml": "^2.8.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../../kb-labs-devkit",
    "@types/node": "^24.3.3",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "eslint": "^9",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
