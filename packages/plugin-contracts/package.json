{
  "name": "@kb-labs/plugin-contracts",
  "version": "0.1.1",
  "description": "Plugin System Contracts - Pure types with 0 runtime dependencies",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@kb-labs/core-platform": "link:../../../kb-labs-core/packages/core-platform",
    "@kb-labs/studio-contracts": "link:../../../kb-labs-studio/packages/studio-contracts"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@types/node": "^24.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "kb-labs",
    "plugin",
    "contracts",
    "v3"
  ],
  "license": "MIT",
  "private": true
}
